{
  "name": "sqy-traffic",
  "version": "0.0.1",
  "description": "Backend API for traffic disruption analysis",
  "main": "src/index.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build && npm run clean"
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node dist/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "clean": "rm -rf dist/ coverage/",
    "lint": "eslint src/** tests/**",
    "lint:fix": "eslint --fix src/** tests/**",
    "test": "jest --coverage tests",
    "test:watch": "jest --coverage tests --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/Epitech/sqy-traffic.git"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "author": "POC Innovation - (Theo Hinsinger, Thomas Michel)",
  "license": "ISC",
  "bugs": {
    "url": "https://https://github.com/Epitech/sqy-traffic/issues"
  },
  "homepage": "https://https://github.com/Epitech/sqy-traffic#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "@typescript-eslint/typescript-estree": "^3.8.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@jest/globals": "^26.3.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/winston": "^2.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "winston": "^3.3.3"
  }
}
